<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="70px"
        android:layout_alignParentTop="true"
        android:layout_marginTop="23px"
        android:padding="10dp">

        <ImageView
            android:id="@+id/iv_project_tip"
            android:layout_width="24px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="53px"
            android:src="@drawable/lamp_project_no_select_tip"
            android:visibility="gone"/>

        <TextView
            android:layout_width="130px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="93px"
            android:gravity="center"
            android:text="项 目 选 择 :"
            android:textColor="#4D4D4D"
            android:textSize="10pt" />

        <com.bete.lamp.customWidget.EditSpinner
            android:id="@+id/spin_shiyan_project"
            android:layout_width="264px"
            android:layout_height="46px"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="227px"
            android:background="@drawable/et_lamp_project_spinner_selector"
            android:gravity="center"
            android:paddingLeft="8dp"
            android:paddingRight="50px"
            android:popupBackground="@color/white"
            android:spinnerMode="dropdown"
            android:text="请选择"
            android:textColor="#4D4D4D"
            android:textSize="8pt" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1.5"
            android:orientation="horizontal"
            android:visibility="invisible">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="质控时间"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/tv_shiyan_step1_qctime"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="无"
                android:textAlignment="center" />
        </LinearLayout>

        <TextView
            android:layout_width="130px"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="640px"
            android:gravity="center"
            android:text="质 控 状 态 :"
            android:textColor="#4D4D4D"
            android:textSize="10pt" />

        <Switch
            android:id="@+id/sw_shiyan_step1_qcenable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="770px"
            android:checked="false"
            android:showText="true"
            android:switchTextAppearance="@style/SwitchTheme"
            android:textOff="关"
            android:textOn="开"
            android:textSize="12pt"
            android:thumb="@drawable/lamp_switch_thumb_selector"
            android:track="@drawable/lamp_switch_track_selector" />

        <CheckBox
            android:id="@+id/cb_shiyan_step1_qcenable"
            android:layout_width="104dp"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="840px"
            android:layout_gravity="center"
            android:background="@drawable/cb_lamp_project_checkbox_selector"
            android:button="@null"
            android:checked="false"
            android:visibility="gone"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="800px"
        android:layout_height="200px"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="130px"
        android:orientation="vertical">

        <RadioGroup
            android:id="@+id/rg_shiyan_step1_kong"
            android:layout_width="match_parent"
            android:layout_height="70px"
            android:layout_gravity="left"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingHorizontal="10dp">

            <TextView
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="" />

            <RadioButton
                android:layout_width="64px"
                android:layout_height="61px"
                android:layout_marginLeft="33px"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/lamp_project_rb_normal"
                android:button="@null"
                android:enabled="false"
                android:textColor="#7D93AC"
                android:text="1"
                android:textSize="12pt" />
            <RadioButton
                android:layout_width="64px"
                android:layout_height="61px"
                android:layout_marginLeft="87px"
                android:layout_gravity="center"
                android:gravity="center"
                android:button="@null"
                android:background="@drawable/lamp_project_rb_normal"
                android:enabled="false"
                android:textColor="#7D93AC"
                android:text="2"
                android:textSize="12pt" />
            <RadioButton
                android:layout_width="64px"
                android:layout_height="61px"
                android:layout_marginLeft="87px"
                android:layout_gravity="center"
                android:gravity="center"
                android:button="@null"
                android:background="@drawable/lamp_project_rb_normal"
                android:enabled="false"
                android:textColor="#7D93AC"
                android:text="3"
                android:textSize="12pt"  />
            <RadioButton
                android:layout_width="64px"
                android:layout_height="61px"
                android:layout_marginLeft="87px"
                android:layout_gravity="center"
                android:gravity="center"
                android:button="@null"
                android:background="@drawable/lamp_project_rb_normal"
                android:enabled="false"
                android:textColor="#7D93AC"
                android:text="4"
                android:textSize="12pt" />

        </RadioGroup>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="65px"
            android:layout_centerHorizontal="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="110px"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp">

            <TextView
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="样 本 编 号 :"
                android:textSize="10pt"
                android:textColor="#4D4D4D"/>

            <EditText
                android:id="@+id/et_sample_no1"
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:background="@drawable/et_lamp_edittext_yuan_selector"
                android:layout_marginRight="5px"
                android:ems="10"
                android:gravity="center"
                android:imeOptions="flagNoExtractUi"
                android:inputType="text|textPersonName"
                android:maxLength="10"
                android:singleLine="true"
                android:textColor="@color/bt_lamp_button_text_color"
                android:textSize="10pt"
                android:text="123456789" />

            <EditText
                android:id="@+id/et_sample_no2"
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:background="@drawable/et_lamp_edittext_yuan_selector"
                android:layout_marginRight="5px"
                android:ems="10"
                android:gravity="center"
                android:imeOptions="flagNoExtractUi"
                android:inputType="text|textPersonName"
                android:maxLength="10"
                android:singleLine="true"
                android:textColor="@color/bt_lamp_button_text_color"
                android:textSize="10pt"
                android:text="123456789"/>

            <EditText
                android:id="@+id/et_sample_no3"
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:background="@drawable/et_lamp_edittext_yuan_selector"
                android:layout_marginRight="5px"
                android:ems="10"
                android:gravity="center"
                android:imeOptions="flagNoExtractUi"
                android:inputType="text|textPersonName"
                android:maxLength="10"
                android:singleLine="true"
                android:textColor="@color/bt_lamp_button_text_color"
                android:textSize="10pt"
                android:text="123456789"/>

            <EditText
                android:id="@+id/et_sample_no4"
                android:layout_width="145px"
                android:layout_height="match_parent"
                android:background="@drawable/et_lamp_edittext_yuan_selector"
                android:layout_marginRight="5px"
                android:ems="10"
                android:gravity="center"
                android:imeOptions="flagNoExtractUi"
                android:inputType="text|textPersonName"
                android:maxLength="10"
                android:singleLine="true"
                android:textColor="@color/bt_lamp_button_text_color"
                android:textSize="10pt"
                android:text="123456789"/>

        </LinearLayout>

    </RelativeLayout>

    <Button
        android:id="@+id/bt_shiyan_step1_run"
        android:layout_width="155px"
        android:layout_height="wrap_content"
        android:background="@drawable/bt_lamp_button_selector"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginRight="90px"
        android:layout_marginBottom="22px"
        android:gravity="center"
        android:text="运行"
        android:textColor="@color/bt_lamp_button_text_color"
        android:textSize="12pt" />
</RelativeLayout>
